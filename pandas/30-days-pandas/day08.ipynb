    "import pandas as pd\n",
    "def valid_emails(users: pd.DataFrame) -> pd.DataFrame:\n",
    "    pattern = r'^[A-Za-z][A-Za-z0-9._-]*@leetcode\\.com$'\n",
    "    return users[users['mail'].str.match(pattern,na=False)]"
   ]
